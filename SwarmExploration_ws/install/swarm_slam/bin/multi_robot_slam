#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'swarm-slam','console_scripts','multi_robot_slam'
__requires__ = 'swarm-slam'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('swarm-slam', 'console_scripts', 'multi_robot_slam')()
    )
